<% content_for :breadcrumb do %>
  Create Quest for deal
  <%= @deal.name %>
<% end %>

<div class="row">
  <div class="col-md-6">
    <div class="card ">
      <div class="card-header card-header-text">
        <div class="card-icon">
          <i class="material-icons">book</i>
        </div>
        <h4 class="card-title">Add New Quest</h4>
      </div>
      <%= render 'form', quest: @quest, deal: @deal %>
    </div>
  </div>
</div>
