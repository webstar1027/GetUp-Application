<% content_for :breadcrumb do %>
  Edit Business Owner
<% end %>
<% content_for :button do %>
  <%= link_to business_owner_path(@business_owner) do %>
  <button class="btn btn-sm btn-info">Show <%= @business_owner.username %></button>
  <% end %>
<% end %>

<div class="row">
  <div class="col-md-12">
    <div class="card ">
      <div class="card-header card-header-rose card-header-text">
        <div class="card-icon">
          <i class="material-icons">account_box</i>
        </div>
        <h4 class="card-title">Edit Business Owner</h4>
      </div>
      <%= render 'form', business_owner: @business_owner, url: admin_business_owner_path(@business_owner) %>
      
    </div>
  </div>
</div>
