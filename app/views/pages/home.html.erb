
  <div class="main">
    <div class="cd-section" id="headers">
      <!--     *********     HEADER 2      *********      -->
      <div class="header-2">
        <div class="page-header header-filter" style="background-image: url('./assets/img/examples/Slider-BG-1.jpg');">
          <div class="container">
            <div class="row">
              <div class="col-md-8 ml-auto mr-auto text-center">
                <h1 class="title"> Explore places. Earn rewards.</h1>
                <h4>Visit places of interests and collect discounts from your favourite airlines, restaurants and retail stores. Your adventure begins right now with Get Up!</h4>
              </div>
              <div class="col-md-10 ml-auto mr-auto">
                <div class="card card-raised card-form-horizontal">
                  <div class="card-body ">
                    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { autocomplete: "off" }) do |f| %>
                      <div class="row">
                        <div class="col-md-3">
                          <div class="form-group">
                            <%= f.text_field :username, placeholder: "Username", class: "form-control" %>
                          </div>
                        </div>
                        <div class="col-md-3">
                          <div class="form-group">
                            <%= f.email_field :email, placeholder: "Company Email", class: "form-control", autocomplete: false %>
                          </div>
                        </div>
                        <div class="col-md-3">
                          <div class="form-group">
                            <%= f.password_field :password, placeholder: "Password", class: "form-control", autocomplete: false %>
                          </div>
                        </div>
                        <div class="col-md-3">
                          <%= f.submit "Sign up", class: "btn btn-primary btn-block" %>
                        </div>
                      </div>
                    <% end %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!--     *********    END HEADER 2      *********      -->
    </div>
